<!DOCTYPE html>
<html>
<head>
  <link rel="stylesheet" href="stylesheets/styles.css">
  <% include ../partials/header_white.ejs %>
  <% include ../partials/database_nav.ejs %>
</head>

<body>

<div class="container">
  <h2>Database Results</h2>

  <!-- <main id="swup" class="transition-wipe"> -->

    <table>
      <th>id</th>
      <th>user_id</th>
      <th>order_id</th>
      <th>complete</th>
      <% results.forEach(function(r) { %>
        <tr>
          <td><%= r.id %></td>
          <td><%= r.user_id %></td>
          <td><%= r.order_id %></td>
          <td><%= r.complete %></td>
        </tr>
      <% }); %>

    </table>
  <!-- </main> -->
</div>

</body>
</html>
