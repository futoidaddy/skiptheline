<!DOCTYPE html>
<html>
<head>
  <link rel="stylesheet" href="stylesheets/styles.css">
  
  <% include ../partials/database_nav.ejs %>
  <style>
    .logout_button {
      width: 100%;
    }
    .logo_img {
      vertical-align: unset;
    }
    body {
      text-align: center;
    }
    
    .container {
      text-align: center;
      align-self: center;
      width: 50%;
      margin-left: 25%;
      margin-top: 5%;
    }

    table {
      margin-right: 0%;
      width: 100%;
    }
  </style>
</head>

<body>

<div class="container">
  <h2>Database Results</h2>

  <!-- <main id="swup" class="transition-wipe"> -->

    <table>
      <th>id</th>
      <th>user_id</th>
      <th>order_id</th>
      <th>complete</th>
      <% results.forEach(function(r) { %>
        <tr>
          <td><%= r.id %></td>
          <td><%= r.user_id %></td>
          <td><%= r.order_id %></td>
          <td><%= r.complete %></td>
        </tr>
      <% }); %>

    </table>
  <!-- </main> -->
</div>

</body>
</html>
