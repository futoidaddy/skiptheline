<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="stylesheets/styles.css">
    <% include ../partials/header_white.ejs %>
    <% include ../partials/database_nav.ejs %>
  </head>

  <body>


    <div class="container">
      <h2>Database Results</h2>
        <table>
          <th>user_id</th>
          <th>username</th>
          <th>password</th>
          <th>authority</th>
          <% results.forEach(function(r) { %>
            <tr>
              <td><%= r.user_id %></td>
              <td><%= r.username %></td>
              <td><%= r.password %></td>
              <td><%= r.authority %></td>
            </tr>
          <% }); %>

        </table>
      <!-- </main> -->
    </div>
  
  </body>
</html>
